{
    "name": "surgems/redirect-urls",
    "description": "Statamic Addon that redirects URLs.",
    "version": "2.0",
    "latestVersion": "2.0",
    "keywords": [
        "surgems",
        "statamic",
        "redirects"
    ],
    "homepage": "https://github.com/surgems/package-development",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Surge Marketing Solutions",
            "email": "help@surgems.co.uk",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "help@surgems.co.uk"
    },
    "autoload": {
        "psr-4": {
            "Surgems\\RedirectUrls\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "statamic": {
            "name": "Redirect Urls",
            "description": "Statamic Addon that redirects URLs."
        },
        "laravel": {
            "providers": [
                "Surgems\\RedirectUrls\\ServiceProvider"
            ]
        }
    },
    "require": {
        "spatie/simple-excel": "^3.1"
    },
    "require-dev": {
        "laravel/pint": "^1.10"
    }
}
